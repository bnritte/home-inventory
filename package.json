{
  "name": "home-inventory",
  "version": "1.0.0",
  "description": "A home inventory app built with Node and React",
  "main": "./server/server.js",
  "engines": {
    "node": "4.5.0",
    "npm": "3.10.7"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon --exec babel-node ./server/server.js",
    "dev": "source .env && run-p serve watch",
    "test": "DATABASE_URL='mongodb://localhost/home-inventory-test' mocha --recursive --compilers jsx:babel-core/register",
    "build-dev": "webpack --progress",
    "build-prod": "NODE_ENV=production webpack -p --progress --bail",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "webpack --watch --progress",
    "heroku-postbuild": "npm run build-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwinmah/home-inventory.git"
  },
  "author": "Edwin Mah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edwinmah/home-inventory/issues"
  },
  "homepage": "https://github.com/edwinmah/home-inventory#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "cheerio": "^0.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "mongoose": "^4.7.2",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^3.1.2",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-datetime": "^2.8.3",
    "react-dom": "^15.4.1",
    "react-dropzone-s3-uploader": "^0.10.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "tachyons": "^4.6.1",
    "uglify-js": "^2.7.5",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-http": "^3.0.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  }
}
