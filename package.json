{
  "name": "home-inventory",
  "version": "1.0.0",
  "description": "A home inventory app built with Node and React",
  "main": "server.js",
  "engines": {
    "node": "4.5.0",
    "npm": "3.10.7"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers jsx:babel-core/register",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr src/index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar src/index.html -c \"npm run build:html\"",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack -p",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr src/assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\"",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwinmah/home-inventory.git"
  },
  "author": "Edwin Mah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edwinmah/home-inventory/issues"
  },
  "homepage": "https://github.com/edwinmah/home-inventory#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "cheerio": "^0.22.0",
    "cpr": "^2.0.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.7.2",
    "morgan": "^1.7.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^3.1.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  }
}
